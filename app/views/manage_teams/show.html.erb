<div id="paged-data-container">
	<div class="tableCon">
	  	<div class="formHdBox">
		    <div class="tableHd">Admin Setting:  <span> </span>View Team</div>
  		</div>
	  	<div class="adminform">
			<div class="left">

				<div class="fldline">
					<div class="label"><b>Name:</b></div>
					<div class="fld"><%= @manage_team.name %></div>
					<div class="clr"></div>
				</div>
				
				<div class="fldline">
					<div class="label"><b>Position:</b></div>
					<div class="fld"><%= @manage_team.position%></div>
					<div class="clr"></div>
				</div>

				<div class="fldline">
					<div class="label"><b>Image:</b></div>
					<div class="fld"><%= image_tag(@manage_team.image, :width => 300) if @manage_team.image.present? %></div>
					<div class="clr"></div>
				</div>

				<div class="fldline">
					<div class="label"><b>Description:</b></div>
					<div class="fld"><%= @manage_team.desc %></div>
					<div class="clr"></div>
				</div>
					 								
			</div>
			
			<div class="clr"></div>
		</div>
	</div>
</div>

<%= link_to 'Edit', edit_manage_team_path(@manage_team) %> |
<%= link_to 'Back', manage_teams_path %>
