<div id="paged-data-container">
<div class="tableCon">
  <div class="formHdBox">
    <div class="tableHd">Where To Buy</div>
    <div class="selectCity"><img src="assets/addnew.png" border="0"><%= link_to 'Add New', new_wheretobuy_path %>
    </div>
  </div>


<div class="paged-data">
    <div class="tabIner">
<table cellpadding="0" cellspacing="0" width="100%" class="table">
  <thead>
    <tr class="tableHdIner alt-row">
      <td class="">S. NO</td>
      <td>Address</td>
      <td>Country</td>
      <td>City</td>
      <td>Working Days</td>
      <td>Working Hours</td>
      <td>Phone No.</td>
      <td>Fax No.</td>
      <td>Email</td>
      <td>Created</td>
      <td>Action</td>
    </tr>
  </thead>

  <tbody>
    <% @wheretobuys.each do |wheretobuy| %>
      <tr class="tableDet">
        <td class="first"><%= wheretobuy.id %></td>
        <td><%= wheretobuy.address.truncate(140) %></td>
        <td><%= wheretobuy.country %></td>
        <td><%= wheretobuy.city %></td>
        <td><%= wheretobuy.w_days %></td>
        <td><%= wheretobuy.w_hours %></td>
        <td><%= wheretobuy.phone_no %></td>
        <td><%= wheretobuy.fax_no %></td>
        <td><%= wheretobuy.email_id %></td>
        <td><%= wheretobuy.created_at%></td>
        <td align="center">
          <%= link_to image_tag("view.gif", :title =>"View"), wheretobuy %>
          <%= link_to image_tag("edit.png", :title => "Edit"), edit_wheretobuy_path(wheretobuy) %>
          <%= link_to image_tag("delete.png", :title => "Delete"), wheretobuy, method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
<br>

</div>
</div>
