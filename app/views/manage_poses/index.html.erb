<div id="paged-data-container">
<div class="tableCon">
  <div class="formHdBox">
    <div class="tableHd">Pose</div>
    <div class="selectCity"><img src="assets/addnew.png" border="0"><%= link_to 'Add Pose', new_manage_pose_path %>
    </div>
  </div>
  <div class="paged-data">
    <div class="tabIner">
      <table cellpadding="0" cellspacing="0" width="100%" class="table">
        <thead>
          <tr class="tableHdIner alt-row">
            <td width="10%" class="first">S. NO</td>
            <td>Pose Title</td>
            <td>Pose Image</td>
            <td>Description</td>
            <td>Author</td>
            <td>Created</td>
            <td>Updated</td>
            <td>Action</td>
          </tr>
        </thead>
        <tbody>
          <% @manage_poses.each do |manage_poses| %>
            <tr class="tableDet">
              <td class="first"><%= manage_poses.id %></td>
              <td><%= manage_poses.title %></td>
              <td><%= image_tag(manage_poses.pose_image, :width => 25, :height =>30) if manage_poses.pose_image.present? %></td>
              <td><%= manage_poses.desc %></td>
              <td><%= manage_poses.author %></td>
              <td><%= manage_poses.created_at %></td>
              <td><%= manage_poses.updated_at %></td>
              <td align="center">
                <%= link_to image_tag("view.gif", :title => "View"), manage_poses %>&nbsp;&nbsp;
                <%= link_to image_tag("edit.png", :title => "Edit"), edit_manage_pose_path(manage_poses) %>&nbsp;&nbsp;
                <%= link_to image_tag("delete.png", :title => "Delete"), manage_poses, method: :delete, data: { confirm: 'Are you sure?' } %>
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
</div>